{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "instagram_clone",
    "slug": "instagram_clone",
    "android": {
      "package": "com.jasonrivera.instagram_clone",
      "permissions": ["android.permission.RECORD_AUDIO"],
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "ios-insta"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3a2c26ac-f894-4849-a026-939421439c22"
      }
    }
  }
}
